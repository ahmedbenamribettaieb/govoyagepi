{% extends 'GoVoyageBundle::layout_template.html.twig' %}

{% block content %}
    <h1>Voyagepersonalise</h1>

    <table>
        <tbody>
            <tr>
                <th>Idvp</th>
                <td>{{ voyagepersonalise.idVp }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ voyagepersonalise.nom }}</td>
            </tr>
            <tr>
                <th>Villedepart</th>
                <td>{{ voyagepersonalise.villeDepart }}</td>
            </tr>
            <tr>
                <th>Villearrive</th>
                <td>{{ voyagepersonalise.villeArrive }}</td>
            </tr>
            <tr>
                <th>Datedepart</th>
                <td>{% if voyagepersonalise.dateDepart %}{{ voyagepersonalise.dateDepart|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datearrive</th>
                <td>{% if voyagepersonalise.dateArrive %}{{ voyagepersonalise.dateArrive|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Nbrparticipant</th>
                <td>{{ voyagepersonalise.nbrParticipant }}</td>
            </tr>
            <tr>
                <th>Hotelfk</th>
                <td>{{ voyagepersonalise.hotelFk }}</td>
            </tr>
            <tr>
                <th>Clientvpfk</th>
                <td>{{ voyagepersonalise.clientVpFk }}</td>
            </tr>
            <tr>
                <th>Event1fk</th>
                <td>{{ voyagepersonalise.event1Fk }}</td>
            </tr>
            <tr>
                <th>Idguidefk</th>
                <td>{{ voyagepersonalise.idGuideFk }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('voyagepersonalise_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('voyagepersonalise_edit', { 'idVp': voyagepersonalise.idVp }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
