<div class="overlay"></div>
<div class="autorize-popup">
    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
        <br>
        <br>
        <br>
        <br>
        <div align="center">
            {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}
        </div>
        <div class="clear"></div>
        <br>
        <br>
        <br>
        <div align="center" >

            <a class="authorize-btn" style="float: none" href="{{ path('fos_user_security_logout') }}">
                {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
            </a>
        </div>

    {% else %}
        <div class="autorize-tabs">
            <a href="#" class="autorize-tab-a current">Connection</a>
            <a href="#" class="autorize-tab-b">S'inscrire</a>
            <a href="#" class="autorize-close"></a>
            <div class="clear"></div>
        </div>
        {% block fos_user_content %}
            <section class="autorize-tab-content">
                <div class="autorize-padding">
                    {{ render(controller('FOSUserBundle:Security:login')) }}
                </div>
            </section>
            <section class="autorize-tab-content">
                <div class="autorize-padding">
                    {{ render(controller('FOSUserBundle:Registration:register')) }}
                </div>
            </section>
        {% endblock fos_user_content %}
    {% endif %}

</div>
